function ListView(){var self=Ti.UI.createView({backgroundColor:'white'});var db=Ti.Database.install('db/r3.sqlite','r3.sqlite');var results=[];var resultSet=db.execute('SELECT * FROM competitions ORDER BY competition ASC');while(resultSet.isValidRow()){results.push({title:resultSet.fieldByName('competition'),details:resultSet.fieldByName('details'),schedule_id:resultSet.fieldByName('schedule_id'),hasChild:true,height:40});resultSet.next();}
resultSet.close();var table=Ti.UI.createTableView({data:results});self.add(table);table.addEventListener('click',function(e){self.fireEvent('competitionSelected',{data:e.rowData});});return self;};module.exports=ListView;