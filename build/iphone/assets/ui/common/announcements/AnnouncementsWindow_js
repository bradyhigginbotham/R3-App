function AnnouncementsWindow(navGroup){var ListView=require('ui/common/announcements/ListView'),DetailView=require('ui/common/announcements/DetailView');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Announcements',navBarHidden:false});var listView=new ListView(),detailView=new DetailView();var detailContainerWindow=Ti.UI.createWindow({title:'Details'});detailContainerWindow.add(detailView);listView.addEventListener('itemSelected',function(e){detailView.fireEvent('itemSelected',e);navGroup.open(detailContainerWindow);});self.add(listView);return self;}
module.exports=AnnouncementsWindow;