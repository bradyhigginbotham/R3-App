function CollegesWindow(navGroup){var ListView=require('ui/common/colleges/ListView'),DetailView=require('ui/common/colleges/DetailView');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Colleges',navBarHidden:false});var listView=new ListView(),detailView=new DetailView();var detailContainerWindow=Ti.UI.createWindow({title:'College Details'});detailContainerWindow.add(detailView);listView.addEventListener('itemSelected',function(e){detailView.fireEvent('itemSelected',e);navGroup.open(detailContainerWindow);});self.add(listView);return self;}
module.exports=CollegesWindow;