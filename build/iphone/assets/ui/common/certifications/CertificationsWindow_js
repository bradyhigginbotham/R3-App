function CertificationsWindow(navGroup)
{var ListView=require('ui/common/certifications/ListView'),DetailView=require('ui/common/certifications/DetailView');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Certifications',navBarHidden:false});var listView=new ListView(),detailView=new DetailView();var detailContainerWindow=Ti.UI.createWindow({title:'Certification Details'});detailContainerWindow.add(detailView);listView.addEventListener('certificationSelected',function(e){detailView.fireEvent('certificationSelected',e);navGroup.open(detailContainerWindow);});self.add(listView);return self;}
module.exports=CertificationsWindow;