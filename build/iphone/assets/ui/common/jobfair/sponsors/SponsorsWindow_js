function SponsorsWindow(navGroup){var ListView=require('ui/common/jobfair/ListView'),DetailView=require('ui/common/jobfair/DetailView');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Sponsors',navBarHidden:false});var listView=new ListView(0),detailView=new DetailView();var listContainerWindow=Ti.UI.createWindow({title:'Sponsors'});self.add(listView);var detailContainerWindow=Ti.UI.createWindow({title:'Sponsor Details',backgroundColor:'#5B718B'});detailContainerWindow.add(detailView);listView.addEventListener('itemSelected',function(e){detailView.fireEvent('itemSelected',e);self.parentTab.open(detailContainerWindow);});var homeButton=Ti.UI.createButton({title:'Home'});self.leftNavButton=homeButton;homeButton.addEventListener('click',function(){navGroup.close(self.tabGroup);});return self;}
module.exports=SponsorsWindow;