function SessionsWindow(navGroup,osname){var ListView=require('ui/common/presentations/sessions/ListView'),DetailWindow=require('ui/common/presentations/sessions/DetailWindow');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Sessions',navBarHidden:false});var listView=new ListView(osname);var listContainerWindow=Ti.UI.createWindow({title:'Sessions'});self.add(listView);listView.addEventListener('sessionSelected',function(e){var detailWindow=new DetailWindow(self.parentTab,e,osname);self.parentTab.open(detailWindow);});var homeButton=Ti.UI.createButton({title:'Home'});self.leftNavButton=homeButton;homeButton.addEventListener('click',function(){navGroup.close(self.tabGroup);});return self;}
module.exports=SessionsWindow;