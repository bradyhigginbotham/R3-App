function SpeakersWindow(navGroup){var ListView=require('ui/common/presentations/speakers/ListView'),DetailView=require('ui/common/presentations/speakers/DetailView');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Speakers',navBarHidden:false});var listView=new ListView(),detailView=new DetailView();var listContainerWindow=Ti.UI.createWindow({title:'Speakers'});self.add(listView);var detailContainerWindow=Ti.UI.createWindow({title:'Speaker Details'});detailContainerWindow.add(detailView);listView.addEventListener('itemSelected',function(e){detailView.fireEvent('itemSelected',e);self.parentTab.open(detailContainerWindow);});var homeButton=Ti.UI.createButton({title:'Home'});self.leftNavButton=homeButton;homeButton.addEventListener('click',function(){navGroup.close(self.tabGroup);});return self;}
module.exports=SpeakersWindow;