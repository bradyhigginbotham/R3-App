function SpeakersWindow(navGroup,osname){var ListView=require('ui/common/presentations/speakers/ListView'),DetailWindow=require('ui/common/presentations/speakers/DetailWindow');var self=Ti.UI.createWindow({backgroundColor:'#ffffff',title:'Speakers',navBarHidden:false});var listView=new ListView();var listContainerWindow=Ti.UI.createWindow({title:'Speakers'});self.add(listView);listView.addEventListener('speakerSelected',function(e){var detailWindow=new DetailWindow(self.parentTab,e,osname);self.parentTab.open(detailWindow);});var homeButton=Ti.UI.createButton({title:'Home'});self.leftNavButton=homeButton;homeButton.addEventListener('click',function(){navGroup.close(self.tabGroup);});return self;}
module.exports=SpeakersWindow;